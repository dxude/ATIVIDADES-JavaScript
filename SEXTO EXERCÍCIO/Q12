function gerarTabuada(numero) {
    console.log(`Tabuada de ${numero}:`);
    for (let i = 1; i <= 10; i++) {
        let resultado = numero * i;
        console.log(`${numero} X ${i} = ${resultado}`);
    }
}

let numeroDesejado = parseInt(prompt("Digite um número de 1 a 10 para gerar a tabuada:"));

if (numeroDesejado >= 1 && numeroDesejado <= 10) {
    gerarTabuada(numeroDesejado);
} else {
    console.log("Número inválido. Digite um número de 1 a 10.");
}
