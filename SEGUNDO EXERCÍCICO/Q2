var numeroSecreto = Math.floor(Math.random() * 100) + 1;
var palpite;

do {
    palpite = parseInt(prompt("Digite seu palpite (entre 1 e 100):"));
    if (palpite < numeroSecreto) {
        console.log("Seu palpite é menor que o número secreto.");
    } else if (palpite > numeroSecreto) {
        console.log("Seu palpite é maior que o número secreto.");
    } else {
        console.log("Parabéns! Você acertou o número secreto:", numeroSecreto);
    }
} while (palpite !== numeroSecreto);
